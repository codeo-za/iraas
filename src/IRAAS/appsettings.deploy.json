{
  "Urls": "#{URLS}",
  "Logging": {
    "LogLevel": {
      "Default": "#{DEFAULT_LOG_LEVEL}",
      "IRAAS": "#{IRAAS_LOG_LEVEL}"
    }
  },
  "Settings": {
    "MaxInputImageSize": "#{MAX_INPUT_IMAGE_SIZE}",
    "MaxOutputImageSize": "#{MAX_OUTPUT_IMAGE_SIZE}",
    "UseDeveloperExceptionPage": "#{USE_DEVELOPER_EXCEPTION_PAGE}",
    "UseHttps": "#{USE_HTTPS}",
    "EnableTestPage": "#{ENABLE_TEST_PAGE}",
    "DomainWhitelist": "#{DOMAIN_WHITELIST}",
    "MaxConcurrency": "#{MAX_CONCURRENCY}",
    "MaxClients": "#{MAX_CLIENTS}",
    "MaxImageFetchTimeInMilliseconds": "#{MAX_IMAGE_FETCH_TIME_IN_MILLISECONDS}",
    "ShareConcurrentRequests": "#{SHARE_CONCURRENT_REQUESTS}",
    "EnableConnectionKeepAlive": "#{ENABLE_CONNECTION_KEEP_ALIVE}",
    "SuppressErrorDiagnostics": "#{SUPPRESS_ERROR_DIAGNOSTICS}",
    "MaxUrlFetchRetries": "#{MAX_URL_FETCH_RETRIES}",
    "Verbose": "#{VERBOSE}"
  },
  "DefaultParameters": {
    "*": {
      "Quantizer": "#{DEFAULT_QUANTIZER}",
      "Quality": "#{DEFAULT_QUALITY}",
      "ResizeMode": "#{DEFAULT_RESIZE_MODE}",
      "GifColorTableMode": "#{DEFAULT_GIF_COLOR_TABLE_MODE}",
      "Sampler": "#{DEFAULT_SAMPLER}"
    }
  }
}